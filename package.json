{
	"name": "kozz-module-maker",
	"version": "0.2.10",
	"description": "Library to create new module for the Kozz-Bot protocol",
	"main": "./dist",
	"license": "MIT",
	"scripts": {
		"build": "babel ./src --out-dir ./dist --extensions .ts,.tsx --copy-files --source-maps",
		"dev": "npm run build -- --watch & node --watch --watch-path=dist --env-file=.env --enable-source-maps dist/debugIndex.js",
		"npmjs": "tsc && bun run build && bun publish"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.28.3",
		"@babel/core": "^7.28.3",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-react": "^7.27.1",
		"@babel/preset-typescript": "^7.27.1",
		"@types/jest": "^29.5.14",
		"babel": "^6.23.0",
		"babel-plugin-module-resolver": "^5.0.2",
		"concurrently": "^7.6.0",
		"kozz-types": "^0.1.22",
		"nodemon": "^2.0.22"
	},
	"dependencies": {
		"arcsecond": "^5.0.0",
		"dotenv": "^16.6.1",
		"socket.io-client": "^4.8.1"
	},
	"resolutions": {
		"glob/minimatch": "7.4.6"
	}
}
